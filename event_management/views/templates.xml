<odoo>
    <data>
      <template id="include_date" inherit_id="website_sale.cart_lines">
        <xpath expr="//table[@id='cart_products']//thead//tr//th" position="before">
          <th class="text-center td-date">Date</th>
        </xpath>
         <xpath expr="//table[@id='cart_products']//tbody//tr//td" position="before">
          <td t-esc="date"/>
        </xpath>
      </template>
    </data>
</odoo>
